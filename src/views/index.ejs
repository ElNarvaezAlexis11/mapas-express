<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Locations</title>
</head>
<body>
    <div class="">
        <section>
            <form action="/" method="get">
                <div class="">
                    <label for="">estados</label>
                    <select name="estado">
                        <% estados.forEach(location => {%>
                            <option><%= location.C_ADMINISTRATIVA %></option>
                        <%})%>
                    </select>
                </div>
                <div class="">
                    <label for="">estados</label>
                    <select name="turno">
                        <% turnos.forEach(location => {%>
                            <option><%= location.C_TURNO_1 %></option>
                        <%})%>
                    </select>
                </div>
                <div class="">
                    <label for="">estados</label>
                    <select name="sost">
                        <% sostenimientos.forEach(location => {%>
                            <option><%= location.SOSTENIMIENTO_C_CONTROL %></option>
                        <%})%>
                    </select>
                </div>
                <button type="submit">enviar</button>
            </form>

        </section>
        <section data-locations="<%= JSON.stringify(locations) %>"></section>
    </div>
</body>
</html>